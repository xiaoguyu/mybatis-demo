<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wjw.project.mapper.StudentMapper">
    <select id="selectByName" resultType="com.wjw.project.entity.Student">
        select * from student where name = #{name}
    </select>
</mapper>